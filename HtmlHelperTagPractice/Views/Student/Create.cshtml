@model HtmlHelperTagPractice.Models.Students
<center>
    
<h3> Create New Student</h3>
</center>

<form asp-action="Create">
    <div class="row">
    <div class="col-6 col">
        <div class="row">
            <label asp-for="Id" class="control-label"></label>
            <input asp-for="Id" class="form-control" />
        </div>
        <div class="row">
            <label asp-for="Name" class="control-label"></label>
            <input asp-for="Name" class="form-control" />
        </div>
        <div class="row">
            <label asp-for="Branch" class="control-label"></label>
            <input asp-for="Branch" class="form-control" />
        </div>

    </div>
    <div class="col-6 col">
        <div class="row">
            <label asp-for="Section" class="control-label"></label>
            <input asp-for="Section" class="form-control" />
        </div>
        <div class="row">
            <label asp-for="Gender" class="control-label"></label>
            <input asp-for="Gender" class="form-control" />
        </div>
    </div>

</div>
<div class="d-grid gap-2">
    <button class="btn btn-primary mt-3">Submit</button>
</div>

</form>